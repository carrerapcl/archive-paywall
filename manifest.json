{
    "manifest_version": 3,
    "name": "Archive Redirector",
    "version": "1.0",
    "description": "Redirect to archive.is version of the current page",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/archive-paywall-icon-nobg-16.png",
        "48": "images/archive-paywall-icon-nobg-48.png",
        "128": "images/archive-paywall-icon-nobg-128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
}
